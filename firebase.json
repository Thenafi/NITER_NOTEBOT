{
  "functions": {
    "source": ".output/server"
  },
  "hosting": [
    {
      "predeploy": ["npm run build && cd .output/server && npm install"],
      "site": "niternotebot",
      "public": ".output/public",
      "cleanUrls": true,
      "rewrites": [
        {
          "source": "**",
          "function": "server"
        }
      ]
    }
  ]
}
