{
  "functions": { "source": ".output/server" },
  "hosting": [
    {
      "predeploy": ["npm run build && cd .output/server && npm install"],
      "site": "niternotebot",
      "public": ".output/public",
      "appAssociation": "AUTO",
      "rewrites": [{ "source": "/beep/**", "dynamicLinks": true }],
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    }
  ]
}
